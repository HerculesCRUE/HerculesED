@*[security|||shared/head/_hojasdeestilo.cshtml|||hercules]*@
@{
    CommunityModel Comunidad = Html.GetComunidad();
    string versionCSS = ViewBag.Version;
    if (Comunidad.VersionCSS.HasValue)
    {
        versionCSS += "-" + Comunidad.VersionCSS.Value.ToString();
    }
    string urlPersonalizacion = ViewBag.BaseURLContent + "/imagenes/proyectos/personalizacion/" + Comunidad.Key.ToString();
}




<!-- MATERIAL ICONS -->
<link type="text/css" rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons&amp;op0ubx" media="all">

<!-- FONT AWESOME -->
<link href="https://use.fontawesome.com/releases/v5.0.8/css/all.css?v=@Html.GetVersion()" rel="stylesheet">

<!-- BOOTSTRAP 4 + PROPELLER -->
<link rel="stylesheet" href="@urlPersonalizacion/theme/libraries/bootstrap/bootstrap.min.css?v=@Html.GetVersion()">
<link rel="stylesheet" href="@urlPersonalizacion/theme/libraries/propeller/css/propeller.min.css?v=@Html.GetVersion()" media="screen" type="text/css">

<!-- LIGHTGALLERY -->
<link type="text/css" rel="stylesheet" href="@urlPersonalizacion/theme/libraries/lightgallery/css/lightgallery.min.css?v=@Html.GetVersion()">

<!-- JQUERY CUSTOM SCROLL BAR Version: 3.1.13 -->
<link type="text/css" rel="stylesheet" href="@urlPersonalizacion/theme/libraries/customscroller/css/jquery.mCustomScrollbar.min.css?v=@Html.GetVersion()">

<!-- PRELOADER MATERIALIZE -->
<link type="text/css" rel="stylesheet" href="@urlPersonalizacion/theme/libraries/spinner-material/spinner-material.css?v=@Html.GetVersion()">

<!-- DATATABLES -->
<link type="text/css" rel="stylesheet" href="@urlPersonalizacion/theme/libraries/datatables/datatables.min.css?v=@Html.GetVersion()">

<!-- SELECT2 -->
<link type="text/css" rel="stylesheet" href="@urlPersonalizacion/theme/libraries/select2/css/select2.min.css?v=@Html.GetVersion()">

<!-- TOASTR - https://www.jqueryscript.net/other/bootstrap-toasts-manager.html -->
<link type="text/css" rel="stylesheet" href="@urlPersonalizacion/theme/libraries/toastr/toastr.min.css?v=@Html.GetVersion()">

<!-- PERSONALIZACION TEMA -->
<link type="text/css" rel="stylesheet" href="@urlPersonalizacion/theme/theme.css?v=@Html.GetVersion()">

<!-- PERSONALIZACION COMUNIDAD -->
<link type="text/css" rel="stylesheet" href="@urlPersonalizacion/theme/community.css?v=@Html.GetVersion()">