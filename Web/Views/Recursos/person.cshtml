                        {
                            <a href="@pPerson.Foaf_homepage" about="@pPerson.Foaf_homepage" target="_blank" property="@pPerson.GetPropertyURI(nameof(pPerson.Foaf_homepage))" class="nombre">Enlace a la web</a>
                        }
                        else
                        {
                            <a>---</a>
                        }
                    }
                </div>
            </div>
            <hr />
            <div class="group-wrap">
                <div class="bloque">
                    <div class="info-proyecto">
                        <div class="bloque">
                            <div class="descripcion">
                                <p>@Html.Translate("CONTACTO")</p>
                            </div>
                        </div>
                    </div>
                    <div class="info-investigador d-flex flex-wrap">
                        <div class="bloque">
                            <div class="label">@Html.Translate("CORREO_ELECTRONICO")</div>
                            @{ if (!string.IsNullOrEmpty(pPerson.Vcard_email) && !string.IsNullOrEmpty(pPerson.GNOSSID))
                                {
                                    <div class="value" about="@pPerson.Vcard_email" property="@pPerson.GetPropertyURI(nameof(pPerson.Vcard_email))"><a href="mailto:@pPerson.Vcard_email" >@pPerson.Vcard_email</a></div>
                                }
                                else
                                {
                                    <div class="value">---</div>
                                }
                            }
                        </div>
                        <div class="bloque">
                            <div class="label">@Html.Translate("TELEFONO")</div>
                            @{ if (!string.IsNullOrEmpty(pPerson.Vcard_hasTelephone) && !string.IsNullOrEmpty(pPerson.GNOSSID))
                                {
                                    <div class="value" about="@pPerson.Vcard_hasTelephone" property="@pPerson.GetPropertyURI(nameof(pPerson.Vcard_hasTelephone))"><a href="tel:@pPerson.Vcard_hasTelephone">@pPerson.Vcard_hasTelephone</a></div>
                                }
                                else
                                {
                                    <div class="value">---</div>
                                }
                            }
                        </div>
                        <div class="bloque">
                            <div class="label">@Html.Translate("DIRECCION_CONTACTO")</div>
                            @{ if (!string.IsNullOrEmpty(pPerson.Vcard_address) && !string.IsNullOrEmpty(pPerson.GNOSSID))
                                {
