<!-- JAVASCRIPT -->
<script type="text/javascript">
    var depuracion = false; // TODO:

    const localUrlBase = "https://localhost:44344/";

    //Funciones propias de la ficha

    // Datos cabeceras de las fichas.
    function CargarDatosCabeceras() {
        var url = "http://serviciosedma.gnoss.com/servicioexterno/Hercules/DatosFichaGrupo"
        if (depuracion) {
            url = localUrlBase + "Hercules/DatosFichaGrupo"
        }
        var arg = {};
        arg.pIdGrupo = "@pGroup.GNOSSID";
        $.get(url, arg, function (data) {
            $("#proyectos-tab div div.data").text(data["Proyectos"]);
            $("#publicaciones-tab div div.data").text(data["Publicaciones"]);
            $("#temas-investigacion-tab div div.data").text(data["Categorias"]);
            $("#miembros-tab div div.data").text(data["Miembros"]);
            $("#colaboradores-tab div div.data").text(data["NumColaboradores"]);
            
            pintarNumeroProyPubl(data);
        });
    }

    // Datos gráfica publicaciones.
    function CargarGraficaPublicaciones(parametros) {
        var url = "http://serviciosedma.gnoss.com/servicioexterno/Hercules/DatosGraficaPublicaciones"
        if (depuracion) {
            url = localUrlBase + "Hercules/DatosGraficaPublicaciones"
        }
        var arg = {};
        arg.pParametros = parametros;

        $.get(url, arg, function (data) {
            $('#chart-publicaciones').remove();
            $('div.chart-publicaciones').append($('<canvas id="chart-publicaciones" class="js-chart"></canvas>'));
            var ctx = document.getElementById('chart-publicaciones');
            var parent = ctx.parentElement;
            var height = parent.offsetHeight;
            ctx.setAttribute('height', 100);
            var myChart = new Chart(ctx, data);
        });
    }


    function pintarNumeroProyPubl(data) {
         $("#nmbr_proyectos").text(data["Proyectos"]);
         $("#nmbr_publicaciones").text(data["Publicaciones"]);
    }


    // Datos gráfica publicaciones.
    function CargarGraficaProyectos(parametros) {
        var url = "http://serviciosedma.gnoss.com/servicioexterno/Hercules/DatosGraficaProyectos"
        if (depuracion) {
            url = localUrlBase + "Hercules/DatosGraficaProyectos"
        }
        var arg = {};
        arg.pParametros = parametros;

        var options = {
            scales: {
                xAxes: [{
                    gridLines: {
                        display: false
                    }
                }],
                yAxes: [{
                    gridLines: {
                        display: true
                    }
                }]
            }
        }

        $.get(url, arg, function (data) {
            $('#chart-proyectos').remove();
            $('div.chart-proyectos').append($('<canvas id="chart-proyectos" class="js-chart"></canvas>'));
            var ctx = document.getElementById('chart-proyectos');
            var parent = ctx.parentElement;
            var height = parent.offsetHeight;
            ctx.setAttribute('height', 100);
            var myChart = new Chart(ctx, data, options);
        });
    }


    // Datos gráfica publicaciones horizontal.
    function CargarGraficaPublicacionesHorizontal(pIdGrupo, parametros) {
        var url = "http://serviciosedma.gnoss.com/servicioexterno/Hercules/DatosGraficaTopicsGrupo"
        if (depuracion) {
            url = localUrlBase + "Hercules/DatosGraficaTopicsGrupo"
        }
        var arg = {};
        arg.pIdGrupo = pIdGrupo;
        arg.pParametros = parametros;
        $.get(url, arg, function (data) {

            // Porcentajes en parte inferior.
            data.options.scales.x.ticks.callback = function (value) { return value + "%" }
            var altura = data.data.labels.length * 50;

            $('#chart-publicaciones-horizontal').remove();
            $('div.chart-publicaciones-horizontal').removeAttr("style");
            $('div.chart-publicaciones-horizontal').css("height", altura + 50);

            $('div.chart-publicaciones-horizontal').append($('<canvas id="chart-publicaciones-horizontal" class="js-chart" width="600" height="' + altura + '"></canvas>'));
            var ctx = document.getElementById('chart-publicaciones-horizontal');
            var parent = ctx.parentElement;
            var width = parent.offsetWidth;
            ctx.setAttribute('width', width);
            var height = parent.offsetHeight;
            ctx.setAttribute('height', height);
            var myChart = new Chart(ctx, data);
        });
    }



    // Datos gráfica colaboradores.
    function CargarGraficaMiembros(pIdGrupo,parametros) {
        var url = "http://serviciosedma.gnoss.com/servicioexterno/Hercules/DatosGraficaMiembrosGrupo"
        if (depuracion) {
            url = localUrlBase + "Hercules/DatosGraficaMiembrosGrupo"
        }
        var arg = {};
        arg.pIdGrupo = pIdGrupo;
        arg.pParametros = parametros;
        $('#graficaCytoscape').empty();
        $.get(url, arg, function (data) {
            $('#graficaCytoscape').empty();

            var cy = window.cy = cytoscape({
                // Contenedor
                container: document.getElementById('graficaCytoscape'),
                // Layout
                layout: {
                    name: 'cose',
                    idealEdgeLength: 100,
                    nodeOverlap: 20,
                    refresh: 20,
                    padding: 30,
                    randomize: false,
                    componentSpacing: 100,
                    nodeRepulsion: 400000,
                    edgeElasticity: 100,
                    nestingFactor: 5,
                    gravity: 80,
                    numIter: 1000,
                    initialTemp: 200,
                    coolingFactor: 0.95,
