<config>
    <ConfiguracionGeneral>
        <namespace>person</namespace>
        <idiomasOnto>
            <idiomaOnto>es</idiomaOnto>
        </idiomasOnto>
        <ocultarTituloDescpImgDoc></ocultarTituloDescpImgDoc>
        <TituloDoc EntidadID="http://xmlns.com/foaf/0.1/Person">http://xmlns.com/foaf/0.1/name</TituloDoc>
        <DescripcionDoc EntidadID="http://xmlns.com/foaf/0.1/Person">http://xmlns.com/foaf/0.1/name</DescripcionDoc>
        <HtmlNuevo>true</HtmlNuevo>
		<CategorizacionTesauroGnossObligatoria>false</CategorizacionTesauroGnossObligatoria>
    </ConfiguracionGeneral>
    <EspefPropiedad>
		<Propiedad  ID="http://w3id.org/roh/crisIdentifier" EntidadID="http://xmlns.com/foaf/0.1/Person">
            <AtrNombre>Identificador</AtrNombre>
            <AtrNombreLectura>Identificador</AtrNombreLectura>
        </Propiedad>
        <Propiedad  ID="http://xmlns.com/foaf/0.1/nick" EntidadID="http://xmlns.com/foaf/0.1/Person">
            <AtrNombre>Firma</AtrNombre>
            <AtrNombreLectura>Firma</AtrNombreLectura>
        </Propiedad>
        <Propiedad  ID="http://xmlns.com/foaf/0.1/firstName" EntidadID="http://xmlns.com/foaf/0.1/Person">
            <AtrNombre>Nombre</AtrNombre>
            <AtrNombreLectura>Nombre</AtrNombreLectura>
        </Propiedad>
        <Propiedad  ID="http://w3id.org/roh/ORCID" EntidadID="http://xmlns.com/foaf/0.1/Person">
            <AtrNombre>ORCID</AtrNombre>
            <AtrNombreLectura>ORCID</AtrNombreLectura>
        </Propiedad>
        <Propiedad  ID="http://xmlns.com/foaf/0.1/name" EntidadID="http://xmlns.com/foaf/0.1/Person">
            <AtrNombre>Nombre completo</AtrNombre>
            <AtrNombreLectura>Nombre completo</AtrNombreLectura>
        </Propiedad>
		<Propiedad  ID="http://vivoweb.org/ontology/core#scopusId" EntidadID="http://xmlns.com/foaf/0.1/Person">
            <AtrNombre>ScopusId</AtrNombre>
            <AtrNombreLectura>ScopusId</AtrNombreLectura>
        </Propiedad>
		<Propiedad  ID="http://w3id.org/roh/isSynchronized" EntidadID="http://xmlns.com/foaf/0.1/Person">
            <AtrNombre>Sincronizado</AtrNombre>
            <AtrNombreLectura>Sincronizado</AtrNombreLectura>
        </Propiedad>
        <Propiedad  ID="http://xmlns.com/foaf/0.1/lastName" EntidadID="http://xmlns.com/foaf/0.1/Person">
            <AtrNombre>Apellidos</AtrNombre>
            <AtrNombreLectura>Apellidos</AtrNombreLectura>
        </Propiedad>
		<Propiedad  ID="http://w3id.org/roh/hasRole" EntidadID="http://xmlns.com/foaf/0.1/Person">
            <AtrNombre>Tiene rol</AtrNombre>
            <AtrNombreLectura>Tiene rol</AtrNombreLectura>
			<SeleccionEntidad>
                <TipoSeleccion>Combo</TipoSeleccion>
                <Grafo>organization.owl</Grafo>
                <UrlTipoEntSolicitada>http://xmlns.com/foaf/0.1/Organization</UrlTipoEntSolicitada>              
                <PropsEntEdicion>
                    <NameProp>http://w3id.org/roh/title</NameProp>
                </PropsEntEdicion>
                <PropsEntLectura>
                    <Propiedad ID="http://w3id.org/roh/title" EntidadID="http://xmlns.com/foaf/0.1/Organization">
                        <AtrNombreLectura></AtrNombreLectura>
                    </Propiedad>
                </PropsEntLectura>
            </SeleccionEntidad>
        </Propiedad>
    </EspefPropiedad>
    <EspefEntidad>
        <Entidad  ID="http://xmlns.com/foaf/0.1/Person">
            <AtrNombre>Persona</AtrNombre>
            <AtrNombreLectura>Persona</AtrNombreLectura>
            <OrdenEntidad>
                <Grupo class="formtheme02">
                    <Grupo class="fieldset">
                        <Grupo Tipo="fieldset" class="mediumLabels">
							<NameProp>http://w3id.org/roh/crisIdentifier</NameProp>
                            <NameProp>http://xmlns.com/foaf/0.1/firstName</NameProp>
							<NameProp>http://xmlns.com/foaf/0.1/lastName</NameProp>
							<NameProp>http://xmlns.com/foaf/0.1/nick</NameProp> 
                            <NameProp>http://w3id.org/roh/ORCID</NameProp> 
							<NameProp>http://vivoweb.org/ontology/core#scopusId</NameProp> 
                            <NameProp>http://xmlns.com/foaf/0.1/name</NameProp> 
							<NameProp>http://w3id.org/roh/hasRole</NameProp>
							<NameProp>http://w3id.org/roh/isSynchronized</NameProp>
                        </Grupo>
                    </Grupo>
                </Grupo>
            </OrdenEntidad>
            <OrdenEntidadLectura>
                <Grupo classLectura="contentGroup contenidoPrincipal">
                    <Grupo classLectura="group title">
                        <Grupo Tipo="titulo">
                            <NameProp>@@@TITULO</NameProp>
                        </Grupo>
                        <Grupo Tipo="subtitulo">
                            <NameProp>@@@SUBTITULO</NameProp>
                        </Grupo>
                    </Grupo>
                </Grupo>
                <Grupo classLectura="group content semanticView">
                    <Grupo classLectura="group group_info">
                        <AtrNombreGrupo>Propiedades</AtrNombreGrupo>
                        <Grupo classLectura="contentGroup">
							<NameProp>http://w3id.org/roh/crisIdentifier</NameProp>
                            <NameProp>http://xmlns.com/foaf/0.1/firstName</NameProp>
							<NameProp>http://xmlns.com/foaf/0.1/lastName</NameProp>
							<NameProp>http://xmlns.com/foaf/0.1/nick</NameProp> 
                            <NameProp>http://w3id.org/roh/ORCID</NameProp> 
							<NameProp>http://vivoweb.org/ontology/core#scopusId</NameProp>
							<NameProp>http://w3id.org/roh/isSynchronized</NameProp>
                            <NameProp>http://xmlns.com/foaf/0.1/name</NameProp>
							<NameProp>http://w3id.org/roh/hasRole</NameProp>
							<NameProp>http://w3id.org/roh/isSynchronized</NameProp>
                        </Grupo>
                    </Grupo>
                </Grupo>
            </OrdenEntidadLectura>
        </Entidad>
    </EspefEntidad>
</config>
